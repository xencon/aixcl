# AIXCL Environment Configuration
# Copy this file to .env and modify values as needed
# The .env file is automatically created from this file on first run

# PostgreSQL Configuration
POSTGRES_USER=admin
POSTGRES_PASSWORD=admin
POSTGRES_DATABASE=webui
POSTGRES_CONTINUE_DATABASE=continue

# pgAdmin Configuration
PGADMIN_EMAIL=admin@example.com
PGADMIN_PASSWORD=admin

# Open WebUI Configuration
OPENWEBUI_EMAIL=admin@example.com
OPENWEBUI_PASSWORD=admin

# Grafana Configuration (optional, defaults shown)
# GRAFANA_ADMIN_USER=admin
# GRAFANA_ADMIN_PASSWORD=admin

# Streaming Configuration (optional, defaults to true)
# FORCE_STREAMING=true

# LLM Council Configuration
# Chairman model - synthesizes final response
CHAIRMAN_MODEL=qwen2.5:14b-instruct

# Council Members - Comma-separated list of models that participate in the council
COUNCIL_MODELS=codegemma:2b,phi3:mini,deepseek-coder:1.3b,qwen2.5:7b

# Default profile for stack commands
PROFILE=sys
